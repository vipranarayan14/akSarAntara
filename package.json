    "eslint": "^4.15.0",
    "husky": "^0.14.3",
    "mocha": "^4.1.0",
    "rimraf": "^2.6.2",
    "vtranslit-scheme-deva": "0.4.0",
    "vtranslit-scheme-itrn": "0.3.0",
    "vtranslit-scheme-knda": "0.3.0",
    "vtranslit-scheme-taml": "0.3.0",
