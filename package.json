  "license": "MIT",
  "keywords": [
    "itrans",
    "transliterator",
    "indian-languages",
    "devanagari",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vipranarayan14/vtranslit.git"
  },
  "bugs": {
    "url": "https://github.com/vipranarayan14/vtranslit/issues"
  },
  "homepage": "https://github.com/vipranarayan14/vtranslit#readme",
  "main": "dist/vtranslit.npm.js",
  "files": [
